#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules/knex/bin/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules/knex/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules/knex/bin/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules/knex/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/knex@2.5.0_better-sqlite3@8.6.0/node_modules:/Users/sebastienlotten/Documents/Projets-git/sound-nation-db/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
